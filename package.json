{
  "name": "line_it_server",
  "version": "1.0.21",
  "description": "Projeto do Atendelo",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "install-app-deps",
    "start": "electron .",
    "clean": "rm -rf ./dist",
    "pack": "build --dir",
    "dist": "build --win --ia32 --x64"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/optiapps/inovefila_central"
  },
  "author": {
    "name": "OptiApps",
    "email": "optiappsbrasil@gmail.com"
  },
  "license": "ISC",
  "homepage": "https://bitbucket.org/optiapps/inovefila_central#readme",
  "dependencies": {
    "electron-log": "^2.2.7",
    "electron-updater": "^2.16.1",
    "express": "^4.15.3",
    "firebase": "^4.1.2",
    "jquery": "^3.2.1",
    "material-components-web": "^0.12.1",
    "mongodb": "^2.2.29",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3",
    "vue": "^2.3.4",
    "vue-router": "^2.5.3"
  },
  "devDependencies": {
    "electron": "^1.6.11",
    "electron-builder": "^19.13.0",
    "electron-packager": "^8.7.2"
  },
  "build": {
    "appId": "br.com.optiapps.line_it",
    "productName": "Line-it - Server",
    "compression": "maximum",
    "linux": {
      "target": "deb",
      "icon": "./icons",
      "depends": [
        "mongodb"
      ],
      "category": "Utility",
      "desktop": "Line-it - Server",
      "publish": [
        {
          "provider": "generic",
          "url": "https://storage.googleapis.com/i9fila.appspot.com/install/linux/central/"
        }
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "lineit.ico",
      "publish": [
        {
          "provider": "generic",
          "url": "https://storage.googleapis.com/i9fila.appspot.com/install/win/central/"
        }
      ]
    }
  }
}
